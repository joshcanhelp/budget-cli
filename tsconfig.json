{
  "include": [
    "src/**/*",
    ".eslintrc.cjs",
    "jest.config.ts"
  ],
  "compilerOptions": {
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "removeComments": true,
    "target": "es2022",
    "module": "es2022",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  }
}